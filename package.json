{
	"name": "@charged-ui/components",
	"version": "1.0.3-alpha.27",
	"description": "Universal web components built with Lit.",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./alert": {
			"types": "./dist/alert/index.d.ts",
			"import": "./dist/alert/index.js"
		},
		"./button": {
			"types": "./dist/button/index.d.ts",
			"import": "./dist/button/index.js"
		},
		"./text": {
			"types": "./dist/text/index.d.ts",
			"import": "./dist/text/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run build:lib",
		"build:lib": "npm run build:types && vite build --config vite.config.lib.ts",
		"build:types": "tsc --project tsconfig.lib.json",
		"build:app": "react-router build",
		"dev": "react-router dev",
		"dev:lib": "vite build --config vite.config.lib.ts --watch",
		"start": "react-router-serve ./build/server/index.js",
		"typecheck": "tsc --noEmit",
		"prepublishOnly": "npm run build:lib"
	},
	"keywords": [
		"web-components",
		"lit",
		"ui-components",
		"react",
		"vue",
		"angular",
		"drupal",
		"wordpress"
	],
	"author": "Your Name",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/yourusername/charged-ui.git"
	},
	"peerDependencies": {
		"lit": "^3.0.0"
	},
	"dependencies": {
		"lit": "^3.3.1"
	},
	"devDependencies": {
		"@heroicons/react": "^2.2.0",
		"@lit/react": "^1.0.8",
		"@react-router/dev": "^7.7.1",
		"@react-router/node": "^7.7.1",
		"@react-router/serve": "^7.7.1",
		"@tailwindcss/vite": "^4.1.4",
		"@types/node": "^20",
		"@types/react": "^19.1.2",
		"@types/react-dom": "^19.1.2",
		"clsx": "^2.1.1",
		"isbot": "^5.1.27",
		"motion": "^12.23.12",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-router": "^7.7.1",
		"shiki": "^3.9.2",
		"tailwindcss": "^4.1.4",
		"typescript": "^5.8.3",
		"vite": "^6.3.3",
		"vite-tsconfig-paths": "^5.1.4"
	}
}
