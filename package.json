{
  "name": "@charged-ui/components",
  "version": "1.0.3-alpha.26",
  "description": "A library of Lit components",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./alert": {
      "types": "./dist/alert.d.ts",
      "import": "./dist/alert.js",
      "require": "./dist/alert.cjs"
    },
    "./aurora": {
      "types": "./dist/aurora.d.ts",
      "import": "./dist/aurora.js",
      "require": "./dist/aurora.cjs"
    },
    "./button": {
      "types": "./dist/button.d.ts",
      "import": "./dist/button.js",
      "require": "./dist/button.cjs"
    },
    "./card": {
      "types": "./dist/card.d.ts",
      "import": "./dist/card.js",
      "require": "./dist/card.cjs"
    },
    "./combobox": {
      "types": "./dist/combobox.d.ts",
      "import": "./dist/combobox.js",
      "require": "./dist/combobox.cjs"
    },
    "./dots": {
      "types": "./dist/dots.d.ts",
      "import": "./dist/dots.js",
      "require": "./dist/dots.cjs"
    },
    "./gallery": {
      "types": "./dist/gallery.d.ts",
      "import": "./dist/gallery.js",
      "require": "./dist/gallery.cjs"
    },
    "./globe": {
      "types": "./dist/globe.d.ts",
      "import": "./dist/globe.js",
      "require": "./dist/globe.cjs"
    },
    "./icon": {
      "types": "./dist/icon.d.ts",
      "import": "./dist/icon.js",
      "require": "./dist/icon.cjs"
    },
    "./icon-selector": {
      "types": "./dist/icon-selector.d.ts",
      "import": "./dist/icon-selector.js",
      "require": "./dist/icon-selector.cjs"
    },
    "./meteors": {
      "types": "./dist/meteors.d.ts",
      "import": "./dist/meteors.js",
      "require": "./dist/meteors.cjs"
    },
    "./modal": {
      "types": "./dist/modal.d.ts",
      "import": "./dist/modal.js",
      "require": "./dist/modal.cjs"
    },
    "./ripple": {
      "types": "./dist/ripple.d.ts",
      "import": "./dist/ripple.js",
      "require": "./dist/ripple.cjs"
    },
    "./stat": {
      "types": "./dist/stat.d.ts",
      "import": "./dist/stat.js",
      "require": "./dist/stat.cjs"
    },
    "./text": {
      "types": "./dist/text.d.ts",
      "import": "./dist/text.js",
      "require": "./dist/text.cjs"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:css": "node tailwind.build.js",
    "build:components": "vite build",
    "build:website": "vitepress build",
    "dev": "npm run watch:css & vitepress dev",
    "preview": "vitepress preview",
    "watch:css": "node tailwind.build.js --watch"
  },
  "keywords": [
    "lit",
    "web-components",
    "library"
  ],
  "author": "Theme Boutique",
  "license": "MIT",
  "directories": {
    "doc": "www"
  },
  "dependencies": {
    "lit": "^3.1.4",
    "motion": "^12.4.3"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "autoprefixer": "^10.4.20",
    "clsx": "^2.1.1",
    "cobe": "^0.6.4",
    "glob": "^10.4.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.10",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "vite": "^5.4.1",
    "vite-plugin-dts": "^4.0.3",
    "vitepress": "^2.0.0-alpha.2",
    "vue": "^3.4.19"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  }
}
