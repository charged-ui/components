{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.109.1",
  "pnpmVersion": "7.33.5",
  "nodeSupportedVersionRange": ">=14.15.0 <15.0.0 || >=16.13.0 <17.0.0 || >=18.15.0 <v21.0.0",
  "projectFolderMinDepth": 1,
  "projectFolderMaxDepth": 2,
  "gitPolicy": {
    "allowedEmailRegExps": ["noreply@charged\\.dev"],
    "sampleEmail": "noreply@charged.dev",
    "versionBumpCommitMessage": "Bump versions [skip ci]",
    "changeLogUpdateCommitMessage": "Update changelogs [skip ci]",
    "changefilesCommitMessage": "Rush change"
  },
  "repository": {
    "url": "https://github.com/microsoft/rush-example"
  },
  "allowedProjectTags": ["documentation", "web-component"],
  "projects": [
    {
      "packageName": "@charged-ui/accordion",
      "projectFolder": "packages/accordion",
      "reviewCategory": "production",
      "tags": ["web-component"],
      "shouldPublish": true
    },
    {
      "packageName": "@charged-ui/disclosure",
      "projectFolder": "packages/disclosure",
      "reviewCategory": "production",
      "tags": ["web-component"],
      "shouldPublish": false
    },
    {
      "packageName": "@charged-ui/tabs",
      "projectFolder": "packages/tabs",
      "reviewCategory": "production",
      "tags": ["web-component"],
      "shouldPublish": false
    },
    {
      "packageName": "@charged-ui/navbar",
      "projectFolder": "packages/navbar",
      "reviewCategory": "production",
      "tags": ["web-component"],
      "shouldPublish": false
    },
    {
      "packageName": "@charged-ui/docs",
      "projectFolder": "apps/docs",
      "reviewCategory": "production",
      "tags": ["documentation"],
      "shouldPublish": false
    }
  ]
}
